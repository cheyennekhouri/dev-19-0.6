<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

    <VBox xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="cs151.application.MainController"
          spacing="10" alignment="CENTER" prefWidth="320" prefHeight="240">
        <Label fx:id="popUp" text="Welcome to KnowledgeTrack Homepage!" />
        <Label fx:id="popUp2" text="Navigate to..." />
        <Label fx:id="popUp3" text="" />

        <Button text="Define Programming Languages"
                onAction="#programmingLang"/>

        <Button text="Student Profile"
            onAction="#studentProfile"/>

        <Button text="Search"
            onAction="#searchProf"/>

        <Button text="Back to Front Page"
            onAction="#onNavigateButtonClick"/>
    </VBox>

